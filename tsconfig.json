{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "module": "es6",
    "target": "es2017",
    "allowJs": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "baseUrl": "./src",
    "paths": {
      "types/*": [
        "types/*"
      ],
      "components/*": [
        "components/*"
      ],
      "utils/*": [
        "utils/*"
      ],
      "constants/*": [
        "constants/*"
      ],
    }
  },
}